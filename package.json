{
  "name": "littlerockchurchapp",
  "version": "1.0.0",
  "description": "church site",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm run start --prefix client",
    "build": "npm run build --prefix client",
    "dev": "concurrently \"nodemon server.js\" \"npm run client\"",
    "heroku-postbuild": "npm install && NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "azure": "npm install && NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasbrown6/LittleRockProd.git"
  },
  "cpu": [
    "x64"
  ],
  "author": "Thomas Brown",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.2.5",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.12",
    "node": "^10.14.1",
    "npm": "^6.11.3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
